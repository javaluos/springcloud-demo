<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luos.mapper.OrderMapper">
    <select id="findById" resultType="com.luos.entity.Order">
        select id, user_id userId, name, price, num from tbl_order where id = #{id}
    </select>
</mapper>